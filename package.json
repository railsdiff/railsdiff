{
  "name": "rails-diff",
  "version": "1.8.0",
  "private": true,
  "description": "Small description for rails-diff goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:eslint:fix": "eslint --cache --ext js,ts . --fix",
    "lint:eslint": "eslint --cache --ext js,ts --max-warnings=0 .",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:prettier:hbs:fix": "prettier --parser glimmer --write \"**/*.hbs\"",
    "lint:prettier:hbs": "prettier --list-different --parser glimmer \"**/*.hbs\"",
    "lint:prettier:js:fix": "prettier --write \"**/*.{js,ts}\"",
    "lint:prettier:js": "prettier --list-different \"**/*.{js,ts}\"",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "prettier": "prettier",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
